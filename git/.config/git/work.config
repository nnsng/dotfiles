[user]
  name = snguyen70
  email = sangngoc.nguyen@dxc.com

[blame]
  coloring = highlightRecent
  date = relative

[init]
  defaultBranch = main

[status]
  branch = true
  short = true
  showStash = true
  showUntrackedFiles = all

[push]
  autoSetupRemote = true
  default = current

[pull]
  rebase = true

[rebase]
  autoStash = true

[url "git@github.dxc.com:"]
  insteadOf = "gh:"
